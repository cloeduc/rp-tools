<?php

namespace CDuc\MainBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PersonnageHasSkill
 */
class PersonnageHasSkill
{
    /**
     * @var \DateTime
     */
    private $timestamp;

    /**
     * @var integer
     */
    private $value;

    /**
     * @var \CDuc\MainBundle\Entity\Personnage
     */
    private $personnage;

    /**
     * @var \CDuc\MainBundle\Entity\Skill
     */
    private $skill;

    /**
     * @var \CDuc\MainBundle\Entity\Topic
     */
    private $topic;


    /**
     * Set timestamp
     *
     * @param \DateTime $timestamp
     * @return PersonnageHasSkill
     */
    public function setTimestamp($timestamp)
    {
        $this->timestamp = $timestamp;

        return $this;
    }

    /**
     * Get timestamp
     *
     * @return \DateTime 
     */
    public function getTimestamp()
    {
        return $this->timestamp;
    }

    /**
     * Set value
     *
     * @param integer $value
     * @return PersonnageHasSkill
     */
    public function setValue($value)
    {
        $this->value = $value;

        return $this;
    }

    /**
     * Get value
     *
     * @return integer 
     */
    public function getValue()
    {
        return $this->value;
    }

    /**
     * Set personnage
     *
     * @param \CDuc\MainBundle\Entity\Personnage $personnage
     * @return PersonnageHasSkill
     */
    public function setPersonnage(\CDuc\MainBundle\Entity\Personnage $personnage)
    {
        $this->personnage = $personnage;

        return $this;
    }

    /**
     * Get personnage
     *
     * @return \CDuc\MainBundle\Entity\Personnage 
     */
    public function getPersonnage()
    {
        return $this->personnage;
    }

    /**
     * Set skill
     *
     * @param \CDuc\MainBundle\Entity\Skill $skill
     * @return PersonnageHasSkill
     */
    public function setSkill(\CDuc\MainBundle\Entity\Skill $skill)
    {
        $this->skill = $skill;

        return $this;
    }

    /**
     * Get skill
     *
     * @return \CDuc\MainBundle\Entity\Skill 
     */
    public function getSkill()
    {
        return $this->skill;
    }

    /**
     * Set topic
     *
     * @param \CDuc\MainBundle\Entity\Topic $topic
     * @return PersonnageHasSkill
     */
    public function setTopic(\CDuc\MainBundle\Entity\Topic $topic = null)
    {
        $this->topic = $topic;

        return $this;
    }

    /**
     * Get topic
     *
     * @return \CDuc\MainBundle\Entity\Topic 
     */
    public function getTopic()
    {
        return $this->topic;
    }
}
