<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CDuc\MainBundle\Entity\PersonnageHasSkill" table="personnage_has_skill">
    <indexes>
      <index name="fk_personnage_has_skill_skill1_idx" columns="skill_id"/>
      <index name="fk_personnage_has_skill_personnage1_idx" columns="personnage_id"/>
      <index name="fk_personnage_has_skill_topic1_idx" columns="topic_id"/>
    </indexes>
    <id name="personnage" association-key="true"/>
    <id name="skill" association-key="true"/>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false"/>
    <field name="value" type="integer" column="value" nullable="false"/>
    <one-to-one field="personnage" target-entity="Personnage">
      <join-columns>
        <join-column name="personnage_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="skill" target-entity="Skill">
      <join-columns>
        <join-column name="skill_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="topic" target-entity="Topic">
      <join-columns>
        <join-column name="topic_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
